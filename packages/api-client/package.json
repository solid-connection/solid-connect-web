{
  "name": "@solid-connect/api-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sync:bruno": "node ./scripts/sync-bruno.mjs",
    "sync:bruno:remote": "BRUNO_SOURCE_MODE=remote node ./scripts/sync-bruno.mjs",
    "build": "pnpm run sync:bruno",
    "typecheck": "tsc --noEmit",
    "codegen:check": "pnpm run sync:bruno && git diff --exit-code src/generated || (echo \"Generated API client is out of sync. Run: pnpm --filter @solid-connect/api-client run sync:bruno\" && exit 1)"
  },
  "exports": {
    ".": "./src/index.ts",
    "./runtime": "./src/runtime/index.ts",
    "./generated": "./src/generated/index.ts",
    "./generated/admin": "./src/generated/admin/index.ts",
    "./generated/apis/*": "./src/generated/apis/*/index.ts",
    "./hooks": "./src/hooks/index.ts"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
